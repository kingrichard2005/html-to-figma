{
  "name": "html-figma",
  "version": "0.3.1",
  "description": "Convert DOM node to Figma Node",
  "repository": {
    "type": "git",
    "url": "https://github.com/sergcen/html-to-figma"
  },
  "scripts": {
    "start": "parcel tests/page/index.html",
    "publish:package": "npm run build:package && cd build && npm publish",
    "build:package": "tsc --project tsconfig.json && cpx README.md build/ && cpx package.json build/",
    "build:tests": "parcel build tests/page/index.ts --no-optimize --dist-dir dist && cpx \"tests/page/stubs/**/*\" dist/stubs",
    "serve:tests": "serve -s tests/page -p 3000",
    "test": "cross-env NO_UPDATE_NOTIFIER=1 npm run build:tests && jest tests --runInBand"
  },
  "author": "Sergei Savelev",
  "license": "MIT",
  "devDependencies": {
    "@figma/plugin-typings": "^1.19.3",
    "@types/jest": "^29.5.2",
    "@types/node": "^15.6.0",
    "cpx": "^1.5.0",
    "cross-env": "^7.0.3",
    "cssnano": "^5.0.6",
    "html-webpack-inline-source-plugin": "^0.0.10",
    "html-webpack-plugin": "^5.3.2",
    "jest": "^29.6.1",
    "jest-dev-server": "^6.0.3",
    "jest-puppeteer": "^11.0.0",
    "mkdirp": "^3.0.1",
    "parcel": "^2.16.3",
    "postcss": "^8.3.2",
    "puppeteer": "^19.11.1",
    "react-docgen-typescript": "^1.22.0",
    "serve": "^14.2.5",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.2.4",
    "typescript": "^4.2.4",
    "webpack": "^5.46.0",
    "webpack-cli": "^4.7.2"
  },
  "prettier": {
    "tabWidth": 4,
    "singleQuote": true
  },
  "dependencies": {
    "file-type": "^12.2.0"
  },
  "jest": {
    "globalSetup": "./tests/setup.js",
    "globalTeardown": "./tests/globalTeardown.js",
    "preset": "jest-puppeteer"
  }
}
